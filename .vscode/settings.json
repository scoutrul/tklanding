{
    "sublimeTextKeymap.promptV3Features": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "window.zoomLevel": 0,
    "auto-close-tag.enableAutoCloseTag": true,
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    "workbench.iconTheme": "vscode-icons",
    "editor.renderIndentGuides": false,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "svgpreview.singletonmode": true,
    "postcssSorting.config": {
        "order": [
            "custom-properties",
            "dollar-variables",
            "declarations",
            "at-rules",
            "rules"
        ],
        "properties-order": [
            "position", "top", "right", "bottom", "left", 
            "display", "flex", "justify-content", "align-items",
            "width", "min-width", "max-width", "min-height", "max-height", "height", "line-height", 
            "padding", "padding-top", "padding-right", "padding-bottom", "padding-left", "margin", "margin-top", "margin-right", "margin-bottom", "margin-left",]
        },
        "svn.ignoreMissingSvnWarning": true
    }
